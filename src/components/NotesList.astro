---
import type { CollectionEntry } from "astro:content";
import NoteCard from "./NoteCard.astro";

interface Props {
	notes: CollectionEntry<"notes">[];
}

const { notes } = Astro.props;
---

<div class="max-w-4xl mx-auto space-y-6">
  {notes.map((note) => (
    <NoteCard note={note} />
  ))}
</div>
